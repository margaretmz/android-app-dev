<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="edu.uw.aad.mzm.sample.gesture" >

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".CommonGesturesActivity"
            android:label="@string/title_activity_common_gesture"
            android:parentActivityName=".MainActivity">
        </activity>
        <activity
            android:name=".CustomGesturesActivity"
            android:label="@string/title_activity_custom_gesture"
            android:parentActivityName=".MainActivity">
        </activity>

    </application>

</manifest>
